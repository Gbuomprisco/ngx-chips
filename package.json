{
  "name": "ng2-tag-input",
  "version": "1.2.0",
  "description": "Tag Input component for Angular 2",
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "build": "webpack && npm run moveassets && npm run ngc",
    "watch": "webpack -- --watch",
    "server": "webpack-dev-server --config webpack.demo.js --inline --colors --progress --port 3010 --content-base demo",
    "start": "npm run server",
    "test": "karma start karma.conf.js",
    "ngc": "ngc -p ./tsconfig.aot.json",
    "moveassets": "rsync -a --include='*.scss' --include='*.html' --include='*.metadata.json' --include='*/' --exclude='*' modules/ dist/",
    "codecov": "codecov",
    "clean": "rm -rf ./dist && rm -rf ./factories"
  },
  "author": {
    "name": "Giancarlo Buomprisco",
    "email": "giancarlopsk@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "devDependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@types/es6-shim": "^0.31.32",
    "@types/jasmine": "^2.5.41",
    "@types/node": "^7.0.3",
    "angular2-template-loader": "0.6.2",
    "autoprefixer": "^6.7.0",
    "awesome-typescript-loader": "3.1.2",
    "codecov": "^2.1.0",
    "core-js": "^2.4.1",
    "css-loader": "^0.27.3",
    "es6-promise": "4.1.0",
    "es6-shim": "0.35.3",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.4",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine": "^2.5.3",
    "karma": "^1.4.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "2.0.3",
    "node-sass": "^4.3.0",
    "postcss-loader": "^1.2.2",
