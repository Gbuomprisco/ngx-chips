    "to-string-loader": "^1.1.5",
    "ts-helpers": "1.1.2",
    "ts-node": "^3.0.2",
    "tslint": "^4.3.1",
    "typescript": "2.2.1",
    "universal-style-loader": "^0.16.3",
    "url-loader": "^0.5.7",
    "webpack": "^2.3.0",
    "webpack-dev-server": "^2.4.0",
    "webpack-merge": "^4.1.0",
    "zone.js": "0.8.5"
  },
  "dependencies": {
    "ng2-material-dropdown": "0.7.7"
  },
  "peerDependencies": {
    "@angular/common": "^4.0.0",
