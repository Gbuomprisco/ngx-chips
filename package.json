  "main": "./dist/ng2-tag-input.bundle.js",
  "browser": {
    "core": "./dist/core",
    "components": "./dist/components/",

    "helpers": "./dist/core/helpers/",
    "styles": "./dist/core/styles/",
    "pipes": "./dist/core/pipes/",

    "dropdown":"./dist/components/dropdown/",
    "icon": "./dist/components/icon/",
    "tag": "./dist/components/tag/",
    "tag-input-form": "./dist/components/tag-input-form/"
  },
  "files": [
    "dist",
    "index.ts",
    "tsconfig.json",
